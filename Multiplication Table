print("\t\t\tMULTIPLICATION TABLE\n")
for i in range(2):
    print(" \t ", end ="")
for i in range(1, 10):
    print("",i, end = "  ")
print("\t")
print("\t ______________________________________________")
print("\t")
for i in range(1,10):
    print("\t", i, "   |", end = " ")
    for j in range(1,10):
        print(format(i*j, "3d"),end = " ")
    print("\n")
